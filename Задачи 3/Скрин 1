player1_move = input('')
player2_move = input('')
def rock_paper_scissors(player1, player2):
    if player1_move == 'Камень' and player2_move == 'Ножницы':
        print('Победил первый игрок')
    elif player1_move == 'Ножницы' and player2_move == 'Камень':
        print('Победил второй игрок')
    elif player1_move == 'Ножницы' and player2_move == 'Бумага':
        print('Победил первый игрок')
    elif player1_move == 'Бумага' and player2_move == 'Ножницы':
        print('Победил второй игрок')
    elif player1_move == 'Камень' and player2_move == 'Бумага':
         print('Победил второй игрок')
    elif player1_move == 'Бумага' and player2_move == 'Камень':
         print('Победил первый игрок')
    elif player1_move == player2_move:
         print('Ничья')
    else:
         print('Повторите ввод')
print(rock_paper_scissors(player1_move, player2_move))
